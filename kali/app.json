{
  "expo": {
    "name": "Kali",
    "slug": "kali",
    "scheme": "kali",
    "description": "Kali is a mobile app that helps you to track your daily steps and get rewards.",
    "privacy": "unlisted",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "backgroundColor": "#ffffffff",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-router"],
    "ios": {
      "bundleIdentifier": "com.KaliNative.ios",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": [],
      "adaptiveIcon": {}
    },
    "androidStatusBar": {},
    "androidNavigationBar": {},
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3cae47f-1ec6-4bd7-9666-553844be17e3"
      }
    }
  }
}
