{
  "expo": {
    "name": "Kali",
    "slug": "kali",
    "scheme": "untitled-app",
    "description": "My app powered by Draftbit",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "backgroundColor": "#ffffffff",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ],
      [
        "./plugins/privacy-info-plugin",
        {
          "NSPrivacyAccessedAPITypes": [
            {
              "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
              "NSPrivacyAccessedAPITypeReasons": [
                "E174.1"
              ]
            },
            {
              "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
              "NSPrivacyAccessedAPITypeReasons": [
                "DDA9.1"
              ]
            },
            {
              "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
              "NSPrivacyAccessedAPITypeReasons": [
                "CA92.1"
              ]
            },
            {
              "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
              "NSPrivacyAccessedAPITypeReasons": [
                "35F9.1"
              ]
            }
          ]
        }
      ],
      "expo-router",
      "expo-localization"
    ],
    "ios": {
      "bundleIdentifier": "com.KaliNative.ios",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": [],
      "adaptiveIcon": {}
    },
    "androidStatusBar": {},
    "androidNavigationBar": {},
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c3cae47f-1ec6-4bd7-9666-553844be17e3"
      }
    }
  }
}
